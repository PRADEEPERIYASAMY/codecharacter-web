{
  "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": [true, "shorthand-first"],
    "interface-name": false,
    "quotemark": [true, "single", "avoid-escape", "jsx-double"],
    "no-namespace": false,
    "arrow-parens": true,
    "no-any": true
  }
}
